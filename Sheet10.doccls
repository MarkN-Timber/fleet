Function ColStr2Num(ByVal colStr As String) As Long
    Dim colNum As Long
    Dim i As Long
 
     colNum = 0
 
    For i = 1 To Len(colStr)
        colNum = colNum * 26 + (Asc(UCase(Mid(colStr, i, 1))) - 64)
    Next
    
    ColStr2Num = colNum

End Function

