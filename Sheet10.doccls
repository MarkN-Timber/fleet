            End If

            ''エラー判定
            If strErrRowMsg <> "" Then
                blnErrFlg = True
                strErrMsg = strErrMsg & "◆ " & fncFormatDigit(strErrRow, " ", 4) & " 行目" & _
                            " （明細No " & fncFormatDigit(CStr(intCntLoop), " ", 3) & ")" & vbCrLf & Left(strErrRowMsg, Len(strErrRowMsg) - 2)
